{
  "Hugo relref shortcode": {
    "prefix": "h-relref",
    "body": ["{{< relref \"${0}\" >}}"],
    "description": "Add a relative permalink to document"
  },
  "Admonition": {
    "prefix": "admonition",
    "body": [
      "!!! ${1|summary,info,tip,success,question,warning,failure,danger,bug,example,quote|} \"${2}\"",
      "",
      "    ${0}"
    ],
    "description": "Add admonition to document"
  },
  "Content tabs": {
    "prefix": "contenttabs",
    "body": [
      "===! \"${1}\"",
      "",
      "    ${2}",
      "",
      "=== \"${3}\"",
      "",
      "    ${0}"
    ],
    "description": "Add content tabs to document"
  },
  "Prettier ignore": {
    "prefix": "prettier-ignore",
    "body": ["<!-- prettier-ignore -->", "${0}"],
    "description": "Add prettier-ignore mark to document"
  },
  "Cite source": {
    "prefix": "cite",
    "body": ["-- <cite>${0}</cite>"],
    "description": "Add cited source to document"
  }
}
